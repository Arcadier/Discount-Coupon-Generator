        <script type="text/javascript" src="./scripts/package.js"></script>

        <script type="text/javascript">
        $('.delivery .onoffswitch input[type="checkbox"]').click(function() {
            if ($(this).prop("checked") == true) {

                $(".unit_options").show();
                $(".unit_list_outer").show();
            } else {
                $(".unit_options").hide();
                $(".unit_list_outer").hide();
            }
        });

        function readURL1(input1) {
            console.log(input1.files[0]);
            if (input1.files && input1.files[0]) {
                var reader = new FileReader();
                reader.onload = function(e) {
                    console.log(e.target);
                    var container = jQuery(".private-setting-marketplace-logo");
                    container.html('');
                    var pImage = "<img src=" + e.target.result + " alt='image'>";
                    container.append(pImage);
                    jQuery(".browse-control-logo [name=file_name1]").val(input1.files[0]['name']);
                }
                reader.readAsDataURL(input1.files[0]);
            }
        }

        function readURL2(input) {
            console.log(input.files[0]);
            if (input.files && input.files[0]) {
                var reader = new FileReader();
                reader.onload = function(e) {
                    jQuery(".browse-control-background [name=file_name2]").val(input.files[0]['name']);
                }
                reader.readAsDataURL(input.files[0]);
            }
        }
        jQuery("#myModal").on('shown.bs.modal', function() {
            jQuery('#modaldialog').html5imageupload({});
        });
        /*fixed canvas size issue while edit*/
        jQuery("#myModal").on('show.bs.modal', function() {
            jQuery(window).resize('trigger');
        });
        jQuery('.model-img-crop a.download').attr('data-dismiss', 'modal');
        jQuery('body').on('click', '.model-img-crop .download', function() {
            var src = jQuery(this).attr('href');
            var name = jQuery(this).attr('download');
            jQuery(".browse-control input[name='file_name']").val(name);
            jQuery(".cover-photo.bg-image-photo").html('');
            jQuery(".cover-photo.bg-image-photo").append("<img src=" + src + " alt=" + name + ">");
            jQuery("#myModal").modal("hide")
            return false;
        });
        $(document).ready(function() {
            $('#coupon_code').keypress(function(e) { var regex = new RegExp("^[a-zA-Z0-9]+$"); var str = String.fromCharCode(!e.charCode ? e.which : e.charCode); if (regex.test(str)) { return true; } e.preventDefault(); return false; });
            $('.pr-text').keypress(function(e) { var regex = new RegExp("^[a-zA-Z0-9]+$"); var str = String.fromCharCode(!e.charCode ? e.which : e.charCode); if (regex.test(str)) { return true; } e.preventDefault(); return false; });
            $("#btnSavePrivateSetting").click(function() {
                $(".private-setting-body1").css("display", "none");
                $(".private-setting-body2").css("display", "block");
            });
            $('.private-setting-click').click(function() {
                $(".private-setting-click.active").removeClass("active");
                $(".private-setting-click span.set-tick-check").removeClass("set-tick-check");
                $(this).addClass('active');
                $(this).find("span").addClass("set-tick-check");
            });
            $(".btn-configure").click(function() {
                $(".private-setting-body1").css("display", "block");
                $(".private-setting-body2").css("display", "none");
            });
        });

        function hide_image_popup() {
            jQuery("#coverdark").fadeOut();
            jQuery("#large_image_popup").fadeOut();
        }
        $('ul.layout-tabs li').click(function() {
            var tab_id = $(this).attr('data-tab');
            $('ul.layout-tabs li').removeClass('layout-current');
            $('.tab-content').removeClass('current');
            $(this).addClass('layout-current');
            $("#" + tab_id).addClass('current');
        });

        jQuery("#no-more-tables").tablesorter({
            headers: {

                4: {

                    sorter: 'dcurrency'

                }

            },

        });

        jQuery(window).bind('scroll', function() {

            jQuery(".sidebar").getNiceScroll().resize();

        });

        jQuery(".mobi-header .navbar-toggle").click(function(e) {

            e.preventDefault();

            jQuery("body").toggleClass("sidebar-toggled");

        });

        jQuery(".navbar-back").click(function() {

            jQuery(".mobi-header .navbar-toggle").trigger('click');

        });

        /*nice scroll */

        jQuery(".sidebar").niceScroll({

            cursorcolor: "#000",

            cursorwidth: "6px",

            cursorborderradius: "5px",

            cursorborder: "1px solid transparent",

            touchbehavior: true

        });


        jQuery(".sidebar .section-links li > a").click(function() {

            jQuery(".sidebar .section-links li").removeClass('active');

            jQuery(this).parents('li').addClass('active');

        });

        $(document).ready(function() {
            jQuery('.btn_delete_act').click(function() {
                jQuery(this).parents("tr").addClass("active");
                jQuery('#DeleteCustomMethod').show();

                jQuery('#cover').show();

            });



            jQuery('#popup_btnconfirm').click(function() {

                jQuery('#DeleteCustomMethod').hide();
                jQuery("tr.active").remove();
                jQuery('#cover').hide();

            });




            jQuery('#popup_btncancel,.close-popup').click(function() {
                jQuery("tr.active").removeClass("active");
                jQuery('#DeleteCustomMethod').hide();

                jQuery('#cover').hide();

            });
            var watermark = 'Members Only';
            $('.txt-watermark').val(watermark).addClass('watermark');

            $('.txt-watermark').blur(function() {
                if ($(this).val().length == 0) {
                    $(this).val(watermark).addClass('watermark');
                }
            });
            $('.txt-watermark').focus(function() {
                if ($(this).val() == watermark) {
                    $(this).val('').removeClass('watermark');
                }
            });
            var watermarklink = 'www.example.com';
            $('.txt-watermark-link').val(watermarklink).addClass('watermark');

            $('.txt-watermark-link').blur(function() {
                if ($(this).val().length == 0) {
                    $(this).val(watermarklink).addClass('txt-watermark-link');
                }
            });
            $('.txt-watermark-link').focus(function() {
                if ($(this).val() == watermarklink) {
                    $(this).val('').removeClass('txt-watermark-link');
                }
            });

        });
        // Struip connect functions

        function popupCoonectStrip_ok() {
            jQuery("#cover").fadeOut();

            jQuery("#connect-edit-btn").hide();
            jQuery("#connect-save-btn").show();
        }
        // End stripe connect functions

        // Set link stripe functions
        function popupConfirm_close(closeid) {
            jQuery("#cover").fadeOut();
            jQuery("#" + closeid).fadeOut();
        }

        function popupConfirm_ok() {
            jQuery("#cover").fadeOut();
            jQuery("#link-stripe-account").fadeOut();

            jQuery("#unit_force").prop("readonly", false);
            jQuery("#edit-btn").hide();
            jQuery("#save-btn").show();
        }

        function MakeUneditable() {
            var e = false;
            jQuery("#unit_force.required").each(function() {
                var val = jQuery(this).val();
                var attr = jQuery(this).attr('id');
                if (jQuery.trim(val) == '') {
                    e = true;
                    jQuery(this).addClass('error-con');
                }

            });
            if (!e) {
                jQuery("#unit_force").prop("readonly", true);
                jQuery("#save-btn").hide();
                jQuery("#edit-btn").show();
            }

        }

        function SaveConfirm() {
            var e = false;
            jQuery("#unit_force .required").each(function() {
                var val = jQuery(this).val();
                var attr = jQuery(this).attr('id');
                if (jQuery.trim(val) == '') {
                    e = true;
                    jQuery(this).addClass('error-con');
                }

            });
            if (!e) {
                jQuery("#cover").fadeIn();
                jQuery("#link-stripe-account").fadeIn();
            }
        }
        // End set live key function
        $(window).load(function() {
                checklimit();
            $("body").on("click", ".fancy-radio", function() {
                    $(".val-change").find('input').val("");
                if ($('#pg_fixed').is(':checked')) {
                    $(".val-change").find("span").addClass("hide");
                    
                } else {
                    $(".val-change").find("span").removeClass("hide");
                    
                }
            });

            $(".val-change").find('input').on('input', function () {
                
                        var value = $(this).val();
                        
                        if ((value !== '') && (value.indexOf('.') === -1)) {
                            
                            $(this).val(Math.max(Math.min(value, 100), -100));
                        }
                    
                });
                    

                
            $("#limited").on("click", function() {
                checklimit();
            });



        });
        function checklimit(){
            if ($("#limited").prop("checked") == false) {
                $("#redeem").attr("disabled", "disabled");
                $("#coupon_code").removeAttr("disabled");

            } else {
                $("#redeem").removeAttr("disabled");
                $("#coupon_code").attr("disabled", "disabled");
            }
        }
        </script>
        <script type="text/javascript" src="http://bootstrap.arcadier.com/adminportal/images/custom-nicescroll.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.0/jquery.validate.js"></script>
        <script type="text/javascript">
        (function() {
            'use strict';
            window.addEventListener('load', function() {
                // Fetch all the forms we want to apply custom Bootstrap validation styles to
                var forms = document.getElementsByClassName('needs-validation');
                // Loop over them and prevent submission
                var validation = Array.prototype.filter.call(forms, function(form) {
                    form.addEventListener('submit', function(event) {
                        if (form.checkValidity() === false) {
                            event.preventDefault();
                            event.stopPropagation();
                        }
                        form.classList.add('was-validated');
                    }, false);
                });
            }, false);
        })();
        </script>
